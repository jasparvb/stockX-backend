{
  "$id": "http://example.com/example.json",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "title": {
      "$id": "/properties/title",
      "type": "string",
      "title": "The Title Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 255,
      "examples": ["hueter"],
      "message": {
        "required": "Title cannot be blank"
    }
    },
    "_token": {
      "$id": "/properties/token",
      "type": "string",
      "title": "The Token Schema ",
      "default": "",
      "examples": [
        "foo123"
      ]
    }
  },
  "required": ["title"]
}
